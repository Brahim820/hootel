<odoo>
    <data noupdate="1">

      <function id="default_generate_divide_rooms_by_capacity"
          model="ir.values" name="set_default"
          eval="('hotel.config.settings', 'divide_rooms_by_capacity', True)"/>

      <function id="default_generate_end_day_week"
          model="ir.values" name="set_default"
          eval="('hotel.config.settings', 'end_day_week', '6')"/>

      <function id="default_generate_type_move"
          model="ir.values" name="set_default"
          eval="('hotel.config.settings', 'type_move', 'normal')"/>

      <function id="default_generate_default_num_days"
          model="ir.values" name="set_default"
          eval="('hotel.config.settings', 'default_num_days', 'month')"/>

    </data>
</odoo>
